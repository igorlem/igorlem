{
  "compilerOptions": {
    "strict": true,
    "outDir": "./dist",
    "jsx": "react-jsx",
    "noImplicitAny": true,
    "module": "es6",
    "target": "es5",
    "allowJs": false,
    "resolveJsonModule": true,
    "allowSyntheticDefaultImports": true,
    "moduleResolution": "Node",
    "sourceMap": true,
    "jsxImportSource": "@emotion/react"
  }
}
